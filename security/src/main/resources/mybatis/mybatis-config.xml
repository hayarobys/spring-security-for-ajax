<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- MyBatis Mapper의 parameterType에서 사용할 별명. parameterMap이 아님에 주의 -->
		<!-- 계정 정보를 옮기는데 사용됩니다. -->
		<typeAlias type="com.suph.security.core.userdetails.jdbc.vo.MemberVO" alias="MemberVO"/>
		
		<!-- 계정 보유 권한을 옮기는데 사용됩니다. -->
		<typeAlias type="com.suph.security.core.userdetails.jdbc.vo.AuthVO" alias="AuthVO"/>
		
		<!-- 리소스 접근에 필요한 권한을 옮기는데 사용됩니다. -->
		<typeAlias type="com.suph.security.core.resourcedetails.jdbc.vo.RoleVO" alias="RoleVO"/>
	</typeAliases>
</configuration>
